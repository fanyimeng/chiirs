	Doi = {10.1051/0004-6361/201423569},
	Eid = {A79},
	Eprint = {1403.2996},
	Journal = {\aap},
	Keywords = {dust, extinction, ISM: clouds, submillimeter: ISM, methods: data analysis, ISM: general, Astrophysics - Astrophysics of Galaxies},
	Month = mar,
