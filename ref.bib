	Keywords = {H II regions, ISM: individual objects: Sagittarius B2, stars: formation},
	Month = feb,
	Number = {2},
	Pages = {L36},
	Title = {{Flickering of 1.3 cm Sources in Sgr B2: Toward a Solution to the Ultracompact H II Region Lifetime Problem}},
	Volume = {781},
	Year = 2014,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {https://doi.org/10.1088/2041-8205/781/2/L36},
