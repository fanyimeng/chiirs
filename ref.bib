	Doi = {10.1051/0004-6361:20054737},
	Eprint = {astro-ph/0603461},
	Journal = {\aap},
	Keywords = {acceleration of particles, shock waves, turbulence, ISM: supernova remnants},
	Month = jul,
	Pages = {181-191},
	Title = {{Turbulence and particle acceleration in collisionless supernovae remnant shocks. I. Anisotropic spectra solutions}},
	Volume = 453,
	Year = 2006,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {https://doi.org/10.1051/0004-6361:20054737},
