@article{Mehringer:1993aa,
	Abstract = {The study reports VL radio continuum and H110-alpha radio recombination line observations of Sgr B2 (G0.7-0.0) with spatial resolutions ranging from 3 to 20 arcsec. The extended component of Sgr B2 is imaged; the flux density of this component accounts for about 50 percent of the total flux density of the region at 20 cm. The discovery of several shell-like and cometary H II regions is reported. Physical parameters such as emission measures and electron densities are derived. The kinematics of the ionized gas is investigated. It is shown that the kinematics of the gas in Sgr B2 are complex. Although most of the ionized gas has velocities in the range 50-70 km/s, there is also some gas at velocities of about 100 km/s. The electron temperatures of sources in Sgr B2 range from 4700 to about 10,000 K.},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
