	Journal = {\aap},
	Keywords = {pulsars: general, stars: winds, outflows, acceleration of particles, radiation mechanisms: non-thermal},
	Month = jan,
	Pages = {A110},
	Title = {{Multiband nonthermal radiative properties of pulsar wind nebulae}},
	Volume = 609,
