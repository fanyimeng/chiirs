	Date-Added = {2018-05-08 08:42:40 +0000},
	Date-Modified = {2018-05-08 08:42:42 +0000},
	Doi = {10.1021/jp311240h},
	Eprint = {1305.1047},
	Journal = {Journal of Physical Chemistry A},
	Month = oct,
	Pages = {9404-9419},
	Title = {{74 MHz Nonthermal Emission from Molecular Clouds: Evidence for a Cosmic Ray Dominated Region at the Galactic Center}},
