	Doi = {10.1051/0004-6361/201628095},
	Eid = {A104},
	Eprint = {1603.03344},
	Journal = {\aap},
	Keywords = {methods: data analysis, methods: statistical, ISM: structure, ISM: clouds, instrumentation: interferometers, dust, extinction, Astrophysics - Instrumentation and Methods for Astrophysics, Astrophysics - Astrophysics of Galaxies},
	Month = may,
	Pages = {A104},
	Primaryclass = {astro-ph.IM},
