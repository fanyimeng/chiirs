	Keywords = {B Stars, Cepheid Variables, H Ii Regions, Herbig-Haro Objects, Radio Sources (Astronomy), Late Stars, Star Formation, Stellar Evolution},
	Month = jan,
	Pages = {331-338},
	Title = {{Is Cepheus A East a Herbig-Haro object?}},
	Volume = 105,
	Year = 1993,
