	Journal = {\aap},
	Keywords = {stars: formation, HII regions, methods: observational, methods: numerical, ISM: structure, Astrophysics - Solar and Stellar Astrophysics, Astrophysics - Astrophysics of Galaxies},
	Month = aug,
