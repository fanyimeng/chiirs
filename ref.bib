	Doi = {10.1093/mnras/255.1.105},
	Journal = {\mnras},
	Keywords = {Galactic Mass, Mass Distribution, Maximum Likelihood Estimates, Milky Way Galaxy, Dwarf Galaxies, Galactic Structure, Globular Clusters, Interstellar Matter, Velocity Distribution, Astrophysics},
	Month = mar,
	Pages = {105-118},
	Title = {{The mass of the Milky Way galaxy.}},
