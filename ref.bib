	Journal = {\mnras},
	Keywords = {open clusters and associations: individual: Trumpler 37, open clusters and associations: individual: Teutsch 74},
	Month = apr,
	Pages = {3206-3216},
	Title = {{The embedded cluster or association Trumpler 37 in IC 1396: a search for evolutionary constraints}},
