	Journal = {\apj},
	Keywords = {Gravitational Effects, Interstellar Magnetic Fields, Interstellar Matter, Magnetic Clouds, Star Formation, Zeeman Effect, Astronomical Models, Correlation, Equilibrium, Gas Pressure, Gravitational Collapse},
	Month = jul,
	Pages = {141-158},
	Title = {{Nonhomologous contraction and equilibria of self-gravitating, magnetic interstellar clouds embedded in an intercloud medium: Star formation. II - Results}},
	Volume = 207,
	Year = 1976,
