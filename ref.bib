	Keywords = {ISM: H II REGIONS, GALAXIES: INDIVIDUAL (HENIZE 2-10), GALAXIES: ISM, GALAXIES: STAR CLUSTERS, RADIO CONTINUUM: GALAXIES, STARS: EARLY-TYPE, ISM: H II Regions, galaxies: individual (Henize 2-10), Galaxies: ISM, galaxies: star clusters, Radio Continuum: Galaxies, Stars: Early-Type},
	Month = dec,
