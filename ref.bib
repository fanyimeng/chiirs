	Date-Modified = {2020-07-07 13:04:10 +0200},
	Doi = {10.1086/346149},
	Eprint = {astro-ph/0206037},
	Journal = {\apj},
	Keywords = {Hydrodynamics, ISM: Clouds, Stars: Formation, Turbulence, Astrophysics},
