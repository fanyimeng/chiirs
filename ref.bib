	Eprint = {astro-ph/9402022},
	Journal = {\mnras},
	Keywords = {STARS: FORMATION, ISM: INDIVIDUAL: CEPHEUS A, ISM: JETS AND OUTFLOWS, RADIO CONTINUUM: ISM},
	Month = jan,
	Pages = {469-480},
	Title = {{High-resolution observations of Cepheus A}},
