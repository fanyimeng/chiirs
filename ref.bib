	Keywords = {STARS: EVOLUTION, STARS: INTERIORS, STARS: PRE-MAIN-SEQUENCE},
	Month = nov,
	Pages = {414},
	Title = {{The Pre-Main-Sequence Evolution of Intermediate-Mass Stars}},
	Volume = 418,
	Year = 1993,
