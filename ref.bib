@article{Schilke:1997aa,
	Abstract = {We study the production of SiO in the gas phase of molecular outflows, through the sputtering of Si-bearing material in grains. The sputtering is driven by neutral particle impact on charged grains in C-type shocks, at the speed corresponding to ambipolar diffusion. Shock speeds in the range 10&lt;v_s_&lt;40km/s and preshock densities 10^4^&lt;n_H_&lt;10^7^cm^-3^ have been investigated. Sputtering of Si-bearing material in both the cores and the mantles of the grains is considered. We find that, for v_s_ of approximately 25km/s and n_H_ of the order 10^5^cm^-3^, column densities of SiO similar to those observed in molecular outflow regions can be generated by either mechanism. Impact by particles heavier than helium dominates the core-sputtering process for shock velocities of this order. The profiles of rotational transitions of SiO are computed and compared with observations of molecular outflows.},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {https://ui.adsabs.harvard.edu/abs/1997A&A...321..293S},
