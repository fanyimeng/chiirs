	Date-Modified = {2020-06-25 21:45:32 +0200},
	Doi = {10.1051/0004-6361:20078900},
	Eprint = {0803.2791},
	Journal = {\aap},
	Keywords = {astrochemistry, atomic processes, magnetohydrodynamics (MHD), molecular processes, radiative transfer, shock waves, Astrophysics},
