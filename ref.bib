@article{Yorke:1983aa,
	Abstract = {Radio continuum 11 cm numerical radiation transfer calculations are performed for H II regions, at various evolutionary times during that phase in which molecular cloud material is evaporated due to ionization and heating by nearby OB stars. The temperature and density structure of the ionized gas is obtained, as a function of time, by means of axially symmetric hydrodynamic calculations of the phase in question for three previously discussed cases. The improved resolution obtained, in conjunction with the solution of the radiation transfer problem, has allowed the construction of theoretical radio continuum maps of the H II regions at various 'observing' angles. The flows of this phase are characterized by low surface brightness and by extended radio emission with a compact component, which will only under special circumstances be coincident with the ionizing source.},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
