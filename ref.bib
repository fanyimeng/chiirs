	Date-Modified = {2017-09-15 23:39:30 +0000},
	Doi = {10.1146/annurev.astro.41.011802.094844},
	Eprint = {astro-ph/0301540},
	Journal = {\araa},
	Pages = {57-115},
	Title = {{Embedded Clusters in Molecular Clouds}},
	Volume = 41,
