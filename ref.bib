	Doi = {10.1088/0034-4885/46/8/002},
	Journal = {Reports on Progress in Physics},
	Keywords = {Energetic Particles, Particle Acceleration, Plasma-Particle Interactions, Rarefied Plasmas, Shock Wave Propagation, Kinematics, Nonlinear Systems, Oblique Shock Waves, Plane Waves, Rankine-Hugoniot Relation, Time Dependence},
	Month = aug,
	Pages = {973-1027},
	Title = {{An introduction to the theory of diffusive shock acceleration of energetic particles in tenuous plasmas}},
	Volume = 46,
