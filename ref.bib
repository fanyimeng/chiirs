	Keywords = {ISM: Clouds, ISM: General, ISM: Structure, Astrophysics},
	Month = apr,
	Number = {1},
	Pages = {278-311},
	Primaryclass = {astro-ph},
	Title = {{Neutral Atomic Phases of the Interstellar Medium in the Galaxy}},
	Volume = {587},
