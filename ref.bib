	Eprint = {1703.03358},
	Journal = {\mnras},
	Keywords = {proper motions, stars: early-type, stars: kinematics and dynamics, H II regions, open clusters and associations: individual: Carina Nebula},
	Month = jun,
	Pages = {2469-2481},
	Primaryclass = {astro-ph.SR},
