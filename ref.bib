	Doi = {10.1086/305152},
	Eprint = {astro-ph/9710124},
	Journal = {\apj},
	Keywords = {ACCELERATION OF PARTICLES, ISM: COSMIC RAYS, DIFFUSION, ELEMENTARY PARTICLES, GALAXY: HALO, GAMMA RAYS: THEORY, ISM: ABUNDANCES, Acceleration of Particles, ISM: Cosmic Rays, Diffusion, Elementary Particles, Galaxy: Halo, Gamma Rays: Theory, ISM: Abundances},
