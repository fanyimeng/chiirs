	Doi = {10.1086/185631},
	Journal = {\apjl},
	Keywords = {Herbig-Haro Objects, Linear Polarization, Orion Constellation, Stellar Magnetic Fields, Synchrotron Radiation, Very Large Array (Vla), Infrared Sources (Astronomy), Radio Sources (Astronomy), Stellar Magnetospheres, Stellar Mass},
	Month = jan,
	Pages = {L61-L64},
	Title = {{Detection of synchrotron emission from a unique HH-like object in Orion}},
	Volume = 348,
