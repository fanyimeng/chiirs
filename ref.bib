	Doi = {10.1086/307483},
	Journal = {\apj},
	Keywords = {ISM: CLOUDS, ISM: KINEMATICS AND DYNAMICS, ISM: MAGNETIC FIELDS, ISM: MOLECULES, MAGNETOHYDRODYNAMICS: MHD, WAVES, ISM: Clouds, ISM: Kinematics and Dynamics, ISM: Magnetic Fields, ISM: Molecules, Magnetohydrodynamics: MHD, Waves},
	Month = aug,
	Pages = {706-713},
	Title = {{Magnetic Fields in Molecular Clouds: Observations Confront Theory}},
