@article{Wouterloot:1980aa,
	Abstract = {Strong, highly circularly polarized emission in the 1665- and 1667-MHz lines of OH arising from a region of star formation in the molecular cloud that produced the Cepheus OB3 association have been detected. The two strongest peaks in both the 1665-MHz and the 1667-MHz lines display one single Zeeman pattern indicative of a magnetic field of 3.5 mG.},
