@article{Curiel:1987aa,
	Abstract = {The present model for the thermal radio continuum emission produced by a shock wave is applied to Pravdo et al.'s (1985) observations at 2, 6, and 20 cm of the HH1-2 system. The spectrum thus obtained is similar to those known in H II regions and can reproduce the HH1-2 spectra; the observed spectrum of the central (and possibly exciting) source, however, can be only partially reproduced. These results suggest that the radio emission of the central source is not produced by a shock wave.},
