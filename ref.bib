	Author = {{Rosolowsky}, E.~W. and {Pineda}, J.~E. and {Kauffmann}, J. and {Goodman}, A.~A.},
	Date-Added = {2017-09-15 23:33:08 +0000},
	Date-Modified = {2017-09-15 23:33:09 +0000},
	Doi = {10.1086/587685},
	Eid = {1338-1351},
	Eprint = {0802.2944},
	Journal = {\apj},
	Keywords = {ISM: clouds, ISM: structure, methods: analytical, techniques: image processing},
	Month = jun,
	Pages = {1338-1351},
	Title = {{Structural Analysis of Molecular Clouds: Dendrograms}},
