	Date-Modified = {2019-01-14 23:51:36 +0100},
	Doi = {10.1086/305478},
	Journal = {\apj},
	Keywords = {STARS: CIRCUMSTELLAR MATTER, ISM: GENERAL, ISM: SUPERNOVA REMNANTS, STARS: SUPERNOVAE: GENERAL, Stars: Circumstellar Matter, ISM: General, ISM: Supernova Remnants, Stars: Supernovae: General},
	Month = apr,
