	Doi = {10.1038/nature06620},
	Eprint = {0801.0442},
	Journal = {\nat},
	Month = feb,
	Pages = {1082-1084},
	Title = {{A minimum column density of 1gcm$^{-2}$ for massive star formation}},
	Volume = 451,
	Year = 2008,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1038/nature06620},
