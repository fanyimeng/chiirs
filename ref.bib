	Keywords = {ISM: CLOUDS, ISM: H II REGIONS, ISM: KINEMATICS AND DYNAMICS, ISM: MOLECULES, Astrophysics},
	Month = sep,
	Pages = {1058-1072},
	Primaryclass = {astro-ph},
	Title = {{A ridge of recent massive star formation between Sgr B2M and Sgr B2N}},
	Volume = {361},
