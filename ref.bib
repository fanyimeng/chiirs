	Date-Modified = {2017-09-15 23:41:18 +0000},
	Doi = {10.1086/304808},
	Journal = {\apj},
	Keywords = {Infrared: Stars, Galaxy: Open Clusters and Associations: Individual: Messier Number: M17, Stars: Early-Type, Stars: Pre-Main-Sequence},
