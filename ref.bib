	Author = {{Battersby}, C. and {Bally}, J. and {Ginsburg}, A. and {Bernard}, J.-P. and {Brunt}, C. and {Fuller}, G.~A. and {Martin}, P. and {Molinari}, S. and {Mottram}, J. and {Peretto}, N. and {Testi}, L. and {Thompson}, M.~A.},
