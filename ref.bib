@article{Curiel:1989aa,
	Abstract = {The paper presents 6-cm, D-configuration VLA observations of the young stars R Mon, V645 Cyg and MWC1080. Extended (5-10 arcsec) radio continuum emission was detected in association with V645 Cyg and MWC1080. It is argued that, in these and other young stellar objects, the radio continuum emission comes from ionized gas. This gas has been ionized by the UV photons produced when a neutral stellar wind shocks against surrounding molecular material. The resulting ionizing rate is comparable to that expected from an early B-type ZAMS star.},
