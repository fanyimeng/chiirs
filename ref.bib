@article{Osterbrock:1959aa,
	Abstract = {New observations are presented of [0 ii] X 3729/X 3726 intensity ratios at selected points in the Orion Nebula, as well as estimates of [0 iij/[O iiij and [0 iij/Hp ratios. A schematized, spherically symmetric model of the nebula is constructed, which reproduces, in the mean, the observed line-intensity ratios. However, the optical surface-brightness data and particularly the radio flux-density observations prove that this model, in which the density varies only slowly with position, cannot be correct but that, instead, there must be extreme density fluctuations in the nebula. Though the model with fluctuations cannot be completely specified from the available data, a particular case is analyzed, in which all the matter is in dense condensations distributed at random in a medium with zero density. For this model the density in the condensations is the same as the density at corresponding points in the model without fluctuations, but only about one-thirtieth of the whole volume is occupied by matter. The total mass of the interstellar matter in this model is about 60 solar masses.},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {https://ui.adsabs.harvard.edu/abs/1959ApJ...129...26O},
	Author = {{Osterbrock}, Donald and {Flather}, Edith},
