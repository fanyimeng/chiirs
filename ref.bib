	Eprint = {astro-ph/0506242},
	Journal = {\apj},
	Keywords = {Acceleration of Particles, Galaxy: Center, Radiation Mechanisms: Nonthermal, Stars: Winds, Outflows, ISM: Supernova Remnants, X-Rays: Diffuse Background},
	Month = feb,
	Pages = {786-796},
