@article{Rodriguez:1989aa,
	Abstract = {The triple radio source in the Serpens star formation region consists of a central object and two lobes with spectral indices characteristic of nonthermal emission. Measurements made at the Arecibo Observatory and the VLA indicate that this triple source is probably associated with the Serpens star-forming region. A weak, circularly polarized 1667 MHz OH maser detected at Arecibo has been observed with the VLA and found to coincide in position to within 1 arcsec with the central source. The position of this central source also falls within the position error ellipsoid of IRAS 18273+0113. VLA observations at 6 cm clearly show that the outer components are moving away from the central source. The angular motion of each component with respect to the central one is 1.3 + or - 0.2 arcsec, implying velocities of 300 + or - 100 km/s at a distance of 500 + or - 200 pc.},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
