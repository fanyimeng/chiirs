	Journal = {\apj},
	Keywords = {Halos, Hydrogen Clouds, Interstellar Gas, Molecular Clouds, Nebulae, Temperature Gradients, Ammonia, Morphology, Red Shift},
	Month = jun,
	Pages = {721-731},
	Title = {{The kinetic temperature gradient and the structure of a thin molecular disk in Cepheus A}},
