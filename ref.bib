	Eid = {37},
	Journal = {\apjs},
	Keywords = {Galaxy: structure, ISM: clouds, ISM: evolution, ISM: kinematics and dynamics, ISM: molecules, stars: formation},
	Month = dec,
	Pages = {37},
	Title = {{Mapping Study of 71 Planck Cold Clumps in the Taurus, Perseus, and California Complexes}},
