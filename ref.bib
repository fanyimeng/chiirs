@article{Crocker:2010aa,
	Abstract = {The amplitude of the magnetic field near the Galactic Centre has been uncertain by two orders of magnitude for several decades. On a scale of ~100 parsecs (pc), fields of ~1,000 microgauss (muG refs 1-3) have been reported, implying a magnetic energy density more than 10,000 times stronger than typical for the Galaxy. Alternatively, the assumption of pressure equilibrium between the various phases of the Galactic Centre interstellar medium (including turbulent molecular gas, the contested `very hot' plasma, and the magnetic field) suggests fields of ~100 muG over ~400 pc size scales. Finally, assuming equipartition, fields of only ~6 muG have been inferred from radio observations for 400 pc scales. Here we report a compilation of previous data that reveals a downward break in the region's non-thermal radio spectrum (attributable to a transition from bremsstrahlung to synchrotron cooling of the in situ cosmic-ray electron population). We show that the spectral break requires that the Galactic Centre field be at least ~50 muG on 400 pc scales, lest the synchrotron-emitting electrons produce too much gamma-ray emission, given other existing constraints. Other considerations support a field of 100 muG, implying that over 10% of the Galaxy's magnetic energy is contained in only <~0.05% of its volume.},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/2010Natur.463...65C},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arxiv.org/abs/1001.1275},
	Author = {{Crocker}, R.~M. and {Jones}, D.~I. and {Melia}, F. and {Ott}, J. and {Protheroe}, R.~J.},
	Date-Added = {2017-09-15 23:45:13 +0000},
	Date-Modified = {2017-09-15 23:45:14 +0000},
	Doi = {10.1038/nature08635},
	Eprint = {1001.1275},
	Journal = {\nat},
	Month = jan,
	Pages = {65-67},
	Primaryclass = {astro-ph.GA},
	Title = {{A lower limit of 50 microgauss for the magnetic field near the Galactic Centre}},
	Volume = 463,
	Year = 2010,
